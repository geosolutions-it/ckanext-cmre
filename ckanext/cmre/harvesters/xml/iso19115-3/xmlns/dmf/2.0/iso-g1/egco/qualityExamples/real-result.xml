<?xml version="1.0" encoding="UTF-8"?>
<gmd:DQ_QuantitativeResult xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.isotc211.org/2005/gmd http://standards.iso.org/itff/PubliclyAvailableStandards/ISO_19139_Schemas/gmd/gmd.xsd">
	<!-- ISO 19103 real is implemented in XML as xs:double.  -->
	<gmd:valueType>
		<!-- NOTE:	The current location of the XML Schema implementation of the xs namespace is http://www.w3.org/2001/XMLSchema.xsd. 
							But, its location is subject to change. A URN is used instead of a URL to prevent any change. 
							urn:dgiwg:metadata:dmf:1.0:xmlns:w3:XMLSchema:2001:double currently corresponds to http://www.w3.org/2001/XMLSchema.xsd#double -->
		<gco:RecordType xlink:href="urn:dgiwg:metadata:dmf:1.0:xmlns:w3:XMLSchema:2001:double">Real</gco:RecordType>
	</gmd:valueType>
	<!-- Many different units of measure may be applicable (see http://www.opengis.net/def/uom/OGC/1.0 for common units) -->
	<gmd:valueUnit xlink:href="http://www.opengis.net/def/uom/OGC/1.0/meter"/>
	<!-- Encoding of a record containing a Real value. Floating and Decimal representations are allowed -->
	<gmd:value>
		<gco:Record xsi:type="xs:double">12.0</gco:Record>
	</gmd:value>
</gmd:DQ_QuantitativeResult>
